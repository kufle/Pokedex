{
  "expo": {
    "name": "Pokedex",
    "slug": "pokemon-wiki",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/pokeball-splash.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/pokeball-splash.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.irawandev.my.id.pokemonwiki"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/pokeball-splash.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/pokeball-splash.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c301cf96-417e-4a17-becc-c287493225e2"
      }
    }
  }
}
